{
  "ignore": [
    "sw.js",
    "sw-simple.js",
    "tests/**",
    "**/node_modules/**",
    "watch/**",
    "rollup.config.js",
    "Gulpfile.js"
  ],
  "plugins": [
    "@babel/plugin-transform-instanceof",
    "@babel/plugin-transform-modules-systemjs"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": 3,
        "targets": "> 0.25%, not dead"
      }
    ]
  ]
}
